/* {"URI":"/{id}"} */



#set($_ = $Status<?>(output/status))
#set($_ = $Data<?>(output/view))
#set($_ = $Metrics<?>(output/metrics))

#set($_ = $Id<string>(path/id).WithPredicate(0, 'equal','t', 'id'))
#set($_ = $DefaultPredicate<bool>(const/value).Value('0').WithPredicate(0,'handler','*schedule.Filter'))


SELECT schedule.*,
cast(schedule.enabled as bool)
FROM (
    SELECT t.*  FROM schedule t
     ${predicate.Builder().CombineOr($predicate.FilterGroup(0, "AND")).Build("WHERE")}
) schedule
